# Configuração da API do Backend
REACT_APP_SERVER_URL=http://localhost:3000

# Configurações de Desenvolvimento
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# Configurações de Timeout
REACT_APP_API_TIMEOUT=10000

# Configurações de Cache
REACT_APP_CACHE_ENABLED=true
REACT_APP_CACHE_TTL=300000

# Configurações de Log
REACT_APP_LOG_LEVEL=debug
REACT_APP_LOG_REQUESTS=true

# Configurações de Performance
REACT_APP_ENABLE_CACHE=true

# Instruções de uso:
# 1. Copie este arquivo para .env: cp env.example .env
# 2. Ajuste as configurações conforme necessário
# 3. Para diferentes ambientes, crie arquivos específicos como .env.development, .env.production
